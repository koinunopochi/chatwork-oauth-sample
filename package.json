{
  "name": "chatwork-oauth-sample",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "start": "tsx --env-file=.env src/index.ts",
    "lint": "oxlint src/ test/ && biome check src/ test/",
    "lint:fix": "oxlint src/ test/ --fix && biome check --write src/ test/",
    "format": "biome format --write src/ test/",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "hono": "^4.11.10",
    "tsx": "^4.21.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/node": "^25.2.3",
    "oxlint": "^1.48.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
